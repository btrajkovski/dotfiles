<templateSet group="other">
  <template name="atoi" value="Integer.parseInt($STRING$)" description="Converts String to Integer" toReformat="false" toShortenFQNames="true">
    <variable name="STRING" expression="" defaultValue="" alwaysStopAt="true" />
